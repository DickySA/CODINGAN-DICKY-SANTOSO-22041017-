void main() {
  // Konversi dari Integer ke String
  int ferdiAge = 21;
  String ageAsString = dickyAge.toString();
  print('Umur Dicky dalam bentuk string: $ageAsString');

  // Konversi dari String ke Integer
  String dickyScore = "100";
  int scoreAsInt = int.parse(dickyScore);
  print('Skor Dicky dalam bentuk integer: $scoreAsInt');

  // Konversi dari Double ke Integer
  double ferdiHeight = 175.5;
  int heightAsInt = dickyHeight.toInt();
  print('Tinggi Dicky dalam bentuk integer: $heightAsInt');

  // Konversi dari Boolean ke String
  bool dickyIsStudent = true;
  String isStudentAsString = dickyIsStudent.toString();
  print('Dicky seorang mahasiswa: $isStudentAsString');

  // Konversi dari String ke Double
  String dickyWeight = "72.5";
  double weightAsDouble = double.parse(dickyWeight);
  print('Berat badan Dicky dalam bentuk double: $weightAsDouble');
}
