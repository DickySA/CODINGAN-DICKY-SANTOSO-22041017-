void main() {
  // Membuat beberapa map
  Map<String, String> person = {};
  var product = Map<String, String>();
  var address = <String, String>{};

  // Menampilkan map kosong
  print('Map person: $person');
  print('Map product: $product');
  print('Map address: $address');

  // Membuat map untuk menyimpan nama
  var name = <String, String>{};
  name['first'] = 'Dicky';
  name['middle'] = 'Iman';
  name['last'] = 'Santoso';

  // Menampilkan nama depan
  print('Nama depan: ${name['first']}');

  // Memperbarui nama tengah
  name['middle'] = 'Iman';
  print('Map name setelah diperbarui: $name');

  // Menghapus nama belakang
  name.remove('last');
  print('Map name setelah menghapus nama belakang: $name');
}
