void main() {
  // Membuat list dengan nama "Dicky" untuk menyimpan daftar buku
  List<String> dickyBooks = ['Belajar Dart', 'Pemrograman Flutter', 'Pengembangan Web'];

  // Menampilkan isi list
  print('Daftar Buku Ferdi: ' + dickyBooks.toString());

  // Menambahkan buku baru
  dickyBooks.add('Dasar-Dasar Algoritma');
  print('Setelah menambahkan buku baru: ' + dickyBooks.toString());

  // Menghapus buku
  dickyBooks.remove('Pemrograman Flutter');
  print('Setelah menghapus buku: ' + dickyBooks.toString());

  // Mengakses elemen list
  print('Buku pertama Dicky: ' + dickyBooks[0]);

  // Menampilkan jumlah buku
  print('Jumlah buku Dicky: ' + dickyBooks.length.toString());
}
