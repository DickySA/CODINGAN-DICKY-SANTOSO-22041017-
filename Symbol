void main() {
  // Membuat simbol
  Symbol nameSymbol = Symbol('DICKY SA');
  Symbol ageSymbol = Symbol('Umur');

  // Menggunakan simbol dalam peta
  Map<Symbol, String> dickyInfo = {
    nameSymbol: 'dicky santoso',
    ageSymbol: '21 tahun',
  };

  // Menampilkan informasi menggunakan simbol
  print('${nameSymbol}: ${dickyInfo[nameSymbol]}');
  print('${ageSymbol}: ${dickyInfo[ageSymbol]}');
}
